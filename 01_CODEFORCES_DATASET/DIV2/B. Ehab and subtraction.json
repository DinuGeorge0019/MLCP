        "#include <bits/stdc++.h>\nusing namespace std;\n\n#define _ << \" _ \" <<\n#define TRACE(x) cout << #x << \" = \" << x << endl\n\ntypedef long long ll;\n\n\n\nint main() {\n    ios_base::sync_with_stdio(false);\n    cin.tie(0);\n\n    int n, k;\n    ll a[100010];\n    ll sum = 0;\n\n    cin >> n >> k;\n    for (int i = 0; i < n; i++) cin >> a[i];\n    sort(a, a + n);\n\n    int t = 0;\n\n    for (int i = 0; i < k; i++) {\n        while (t < n && a[t] == sum) t++;\n        if (t == n) cout << \"0\\n\";\n        else {\n            cout << a[t] - sum << \"\\n\";\n            sum = a[t];\n        }\n    }\n\n\n\n\n    \n\n    return 0;\n}\n\n"
